{
    "name": "magentron/laravel-html",
    "description": "A fluent html builder",
    "keywords": [
        "spatie",
        "magentron",
        "laravel",
        "html"
    ],
    "homepage": "https://github.com/magentron/laravel-html",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Jeroen Derks",
            "email": "jeroen@derks.it",
            "homepage": "https://www.phpfreelancer.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/http": "~5.4.0|~5.5.0|~5.6.0",
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.6|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Html\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Html\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Html\\HtmlServiceProvider"
            ],
            "aliases": {
                "Html": "Spatie\\Html\\Facades\\Html"
            }
        }
    }
}
